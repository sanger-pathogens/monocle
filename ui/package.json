    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@testing-library/react-hooks": "^3.3.0",
    "@testing-library/user-event": "^12.0.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.9",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-utilities": "^1.3.4",
    "file-saver": "^2.0.2",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "history": "^4.10.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.0",
    "react-scrollable-anchor": "^0.6.1",
    "react-test-renderer": "^16.13.1",
    "typeface-inter": "^3.12.0"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$VERSION react-scripts start",
    "build": "REACT_APP_VERSION=$VERSION react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:ci": "SKIP_PREFLIGHT_CHECK=true CI=true react-scripts test --env=jest-environment-jsdom-sixteen --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/index.js",
      "!src/serviceWorker.js"
    ]
  }
}
